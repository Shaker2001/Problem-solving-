#include <iostream>
#include<bits/stdc++.h>
#define ll long long
#define for(int,i,n)   for(int i=0;i<n;i++)
using namespace std;

int main()
{
ll n;
cin>>n;
int a [n];
for(int,i,n)
    cin>>a[i];
  int mini=a[0];
  int counter=0;
 for (int,i,n)
 {
     if (mini>a[i])
     {
         mini=a[i];
     }
 }
 for (int,i,n)
 {
     if (mini==a[i])
     {
         counter++;
     }
 }
 if (counter%2==0)
    cout<<"Unlucky";
 else
    cout<<"Lucky";
     return 0;
}



